<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <rect width="600" height="400" fill="#EAEFEF"/>
  <g transform="translate(300, 200)">
    <!-- Search interface -->
    <rect x="-150" y="-80" width="300" height="50" rx="25" fill="white" stroke="#B8CFCE" stroke-width="2"/>
    <circle cx="120" cy="-55" r="15" fill="#7F8CAA"/>
    <line x1="130" y1="-45" x2="140" y2="-35" stroke="#7F8CAA" stroke-width="3"/>
    
    <!-- Multimodal elements -->
    <!-- Text -->
    <rect x="-120" y="-10" width="60" height="40" rx="8" fill="#B8CFCE" opacity="0.7"/>
    <text x="-90" y="15" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#333446">Text</text>
    
    <!-- Voice -->
    <circle cx="-30" cy="10" r="25" fill="#7F8CAA" opacity="0.7"/>
    <path d="M-40,10 L-35,5 L-35,15 Z" fill="white"/>
    <path d="M-30,10 Q-25,5 -20,10 Q-25,15 -30,10" stroke="white" stroke-width="2" fill="none"/>
    
    <!-- Visual -->
    <rect x="30" y="-10" width="60" height="40" rx="8" fill="#333446" opacity="0.7"/>
    <circle cx="45" cy="5" r="8" fill="#EAEFEF"/>
    <rect x="55" y="-5" width="25" height="20" fill="#EAEFEF"/>
    
    <!-- Results -->
    <rect x="-140" y="50" width="280" height="80" rx="8" fill="white" stroke="#B8CFCE" stroke-width="1" opacity="0.9"/>
    <circle cx="-120" cy="70" r="8" fill="#7F8CAA"/>
    <line x1="-100" y1="70" x2="0" y2="70" stroke="#7F8CAA" stroke-width="2"/>
    <line x1="-100" y1="85" x2="50" y2="85" stroke="#B8CFCE" stroke-width="1"/>
    <line x1="-100" y1="100" x2="80" y2="100" stroke="#B8CFCE" stroke-width="1"/>
    <line x1="-100" y1="115" x2="30" y2="115" stroke="#B8CFCE" stroke-width="1"/>
  </g>
  
  <text x="300" y="350" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#333446">
    Multimodal Search Interface
  </text>
</svg>
